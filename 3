
#include <iostream>

using namespace std;

int main()
{
    int K,n;
    cout<<"podaj ilosc kszek";
    cin >> n;
   if (n > 1000) {  
     K=10*n;   
    
} else if (n < 1000 and n > 500) {
    K=12*n;
    
} else if (n < 500 and n > 0) { 
    K=15*n;
    
}else
K=0*n;
cout << "Koszt wydrk " << n << " kszek: " << K;

 
    return 0;
}
